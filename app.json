{
  "expo": {
    "name": "CoHeeApp",
    "slug": "coheeapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "coheeapp",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6F4E37"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cohee.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6F4E37"
      },
      "package": "com.cohee.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
